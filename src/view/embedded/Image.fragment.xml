<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	
	<CustomListItem
		class="sapUiTinyMarginEnd">
			<!-- densityAware="true" todo asdf -->
	    <Image
			height="90px"
			src="{
					path: 'events>id',
					formatter: '.formatter.getSrcImage120px'
					}"	>
	    	
	    	<detailBox>
				<LightBox>
					<LightBoxItem 
						imageSrc="{
								path: 'events>id',
								formatter: '.formatter.getSrcImage'
								}"	
						alt="{plants>plant_name}"
						title="{plants>plant_name}"
						subtitle="">
					</LightBoxItem>
				</LightBox>
			</detailBox>
			
		</Image>
	
	</CustomListItem>

</core:FragmentDefinition>